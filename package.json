    "@types/node": "^17.0.44",
    "@types/oracledb": "^5.2.3",
    "@types/toobusy-js": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.0",
    "chalk": "4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "eslint": "^8.21.0",
    "express": "^4.18.1",
    "express-status-monitor": "^1.3.4",
    "helmet": "^5.1.0",
