    "reflect-metadata": "^0.1.13",
    "toobusy-js": "^0.5.1",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^9.0.1",
    "nodemon": "^2.0.19",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
