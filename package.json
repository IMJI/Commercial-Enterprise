  "scripts": {
    "start": "tsc && node ./bin/index.js",
    "dev": "npm run prettier && npm run lint && tsc && node ./bin/index.js",
    "watch": "nodemon",
    "clear:logs": "del /S logs",
    "clear:dist": "del /S dist",
    "env:prod": "break > ./.env && echo NODE_ENV=production > ./.env",
    "env:dev": "break > ./.env && echo NODE_ENV=development > ./.env",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --config .prettierrc src/**/*.ts --write"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-status-monitor": "^1.3.0",
